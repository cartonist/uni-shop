swiper {
  height: 330rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav-list {
  display: flex;
  justify-content: space-around;
  margin: 15rpx 0;
}
.nav-list image {
  width: 128rpx;
  height: 140rpx;
}
.floor-list {
  padding-left: 10rpx;
}
.floor-list .floor-title {
  width: 100%;
  height: 60rpx;
}
.floor-list .floor-body {
  display: flex;
}
.floor-list .floor-body-right {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
